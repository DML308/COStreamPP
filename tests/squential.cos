composite Main() {
    stream<double x> In, Out, Y;
    (In, Y) = Start() {
        int i = 0;
        work {
            for(i = 0; i < 4; i++) {
                In[i].x = 1;
            }
            
            Y[0].x = 0;
        }
        window {
            In tumbling(4);
            Y tumbling(1);
        }
    };
    Out = squential (In, Y) (4) {
        add Dense(3);
        add Dense(2);
        add Dense(1);
    };
    sink(Out) {
        work {
            print(Out[0].x);
        }
        window{
            Out sliding(1, 1);
        }
    };
}