composite dTest(input stream<int i>In, output stream<int i>Out) {
	Out = dtest (In) {
		work {
			printf("d: ");
			printf(In[0].i);
			Out[0].i = In[0].i;
		}
	};
}
composite rTest(input stream<int i>In, output stream<int i>Out) {
	Out = rtest(In) {
		work {
			printf("r: ");
			printf(In[0].i);
			Out[0].i = In[0].i;
		}
	};
}
composite Main () {
	stream<int i>data, x, y;
	data = start () {
		int i = 0; 
		work {
			i++;
			if (i == 32) {
				i = 0;
			}
			data[0].i = i;
		}
		window {
			data sliding(1, 1);
		}
	};
	y = splitjoin (data) {
		int i; 
		split duplicate();
			for (i = 0; i < 32; i++) {
				add splitjoin {
					split roundrobin();
					for (i = 0; i < 32; i++) {
						add rTest();
					}
					join roundrobin();
				}
			}
		join roundrobin();
	};

	sink(y) {
		int i;
		work {
			for (i = 0; i < 1; i++) {
				printf("sink: ");
				printf(y[i].i);
			}
		}
		window {
			y sliding (1, 1);
		}
	};
}
